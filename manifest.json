{
	"manifest_version": 2,
	"name": "ClearNet",
	"version": "0.1 Alpha",
	"author": "Jolteonshifter",
	"description": "A tool designed to provide a clean, distraction free enviroment aimed to allow people to focus with ease",
	"permission": "storage",
        "icons": {
		"48": "icons/bar.jpg"
	},

        "browser_action": {
          "default_icon": "icons/ClearNet-32",
          "default_popup": "settings/site-lister.html"
        },

	"content_scripts": [
	        {
                  "matches": ["<all_urls>"], 
                  "js": ["scripts/blocker.js"]
		}
	]
}
